@inherits Umbraco.Web.Mvc.UmbracoTemplatePage
@using ContentModels = Umbraco.Web.PublishedContentModels;
@{
	Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <!-- The above 3 meta tags *must* come first in the head; any other head content must come *after* these tags -->
    <title>Web Developer</title>
    <link href="~/Style/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Style/animations.css" rel="stylesheet" />
    <link href="~/Style/component.css" rel="stylesheet" />
    <link href="~/Style/default.css" rel="stylesheet" />
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div class="transpage-top clearfix">
        <div class="container">
            <nav class="navbar navbar-fixed-top">
                <div class="container nav-container">
                    <button class="navbar-toggle" data-target=".navbar-responsive-collapse" data-toggle="collapse" type="button">
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                        <span class="icon-bar"></span>
                    </button>
                    <div class="collapse navbar-collapse navbar-responsive-collapse">
                        <ul class="nav navbar-nav row">
                            <li class="col-sm-3 navbar-block"><a href="#" class="navbar-link home-link current" id="home-link">Home</a></li>
                            <li class="col-sm-3 navbar-block"><a href="#" class="navbar-link about-link" id="about-link">About Me</a></li>
                            <li class="col-sm-3 navbar-block"><a href="#" class="navbar-link services-link" id="services-link">Services</a></li>
                            <li class="col-sm-3 navbar-block"><a href="#" class="navbar-link contact-link" id="contact-link">Contact</a></li>
                        </ul>
                    </div>
                </div><!--close container-->
            </nav>
        </div><!--close navbar container-->
    </div>
        @RenderBody()
    <div class="pt-message">
        <p>Your browser does not support CSS animations.</p>
    </div>
    <script src="~/Scripts/modernizr.js"></script>
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.4/jquery.min.js"></script>
    <!-- Latest compiled and minified JavaScript -->
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    <script src="~/Scripts/animations.js"></script>
</body>
</html>